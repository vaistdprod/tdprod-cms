{
  "name": "multi-tenant",
  "version": "1.0.0",
  "description": "An example of a multi tenant application with Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation NODE_ENV=production next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation NODE_ENV=development next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:schema": "payload-graphql generate:schema",
    "generate:types": "payload generate:types",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation PAYLOAD_DISABLE_PLUGINS=true payload",
    "seed": "cross-env NODE_OPTIONS=--no-deprecation NODE_ENV=development payload migrate:fresh",
    "seed:force": "cross-env NODE_OPTIONS=--no-deprecation NODE_ENV=development echo y | npm run payload migrate:fresh",
    "start": "cross-env NODE_OPTIONS=--no-deprecation NODE_ENV=production next start"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "latest",
    "@payloadcms/next": "latest",
    "@payloadcms/plugin-multi-tenant": "^3.23.0",
    "@payloadcms/richtext-lexical": "latest",
    "@payloadcms/ui": "latest",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "framer-motion": "^12.4.7",
    "graphql": "^16.9.0",
    "next": "^15.0.0",
    "payload": "latest",
    "qs-esm": "7.0.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "0.33.5",
    "swiper": "^11.2.4"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@payloadcms/graphql": "latest",
    "@radix-ui/react-slot": "^1.1.2",
    "@swc/core": "^1.6.13",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.4",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "autoprefixer": "^10.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.0",
    "lucide-react": "^0.475.0",
    "postcss": "^8.0.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^3.0.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.16.2",
    "typescript": "5.5.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "packageManager": "pnpm@10.4.0+sha512.6b849d0787d97f8f4e1f03a9b8ff8f038e79e153d6f11ae539ae7c435ff9e796df6a862c991502695c7f9e8fac8aeafc1ac5a8dab47e36148d183832d886dd52"
}
